{% extends 'base.html.twig' %}

{% block title %}PropertyImage{% endblock %}

{% block body %}
    <h1>PropertyImage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ property_image.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ property_image.createdAt ? property_image.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ property_image.updatedAt ? property_image.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ property_image.image }}</td>
            </tr>
            <tr>
                <th>IsMain</th>
                <td>{{ property_image.isMain ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_property_image_index') }}">back to list</a>

    <a href="{{ path('app_property_image_edit', {'id': property_image.id}) }}">edit</a>

    {{ include('property_image/_delete_form.html.twig') }}
{% endblock %}
