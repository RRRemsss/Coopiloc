{% extends 'base.html.twig' %}

{% block title %}LeaseParty{% endblock %}

{% block body %}
    <h1>LeaseParty</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ lease_party.id }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ lease_party.color }}</td>
            </tr>
            <tr>
                <th>LeasePartyType</th>
                <td>{{ lease_party.leasePartyType }}</td>
            </tr>
            <tr>
                <th>Civility</th>
                <td>{{ lease_party.civility }}</td>
            </tr>
            <tr>
                <th>DateOfBirth</th>
                <td>{{ lease_party.dateOfBirth ? lease_party.dateOfBirth|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>PlaceOfBirth</th>
                <td>{{ lease_party.placeOfBirth }}</td>
            </tr>
            <tr>
                <th>Profession</th>
                <td>{{ lease_party.profession }}</td>
            </tr>
            <tr>
                <th>MonthlyIncome</th>
                <td>{{ lease_party.monthlyIncome }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_lease_party_index') }}">back to list</a>

    <a href="{{ path('app_lease_party_edit', {'id': lease_party.id}) }}">edit</a>

    {{ include('lease_party/_delete_form.html.twig') }}
{% endblock %}
