{% extends 'base.html.twig' %}

{% block title %}IdentityLeaseParty index{% endblock %}

{% block body %}
    <h1>IdentityLeaseParty index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>IdentityDocumentType</th>
                <th>IdentityNumber</th>
                <th>ExpirationDate</th>
                <th>UploadIdentityPath</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for identity_document in identity_documents %}
            <tr>
                <td>{{ identityLeaseParty.id }}</td>
                <td>{{ identityLeaseParty.createdAt ? identity_document.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ identityLeaseParty.updatedAt ? identity_document.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ identityLeaseParty.identityDocumentType }}</td>
                <td>{{ identityLeaseParty.identityNumber }}</td>
                <td>{{ identityLeaseParty.expirationDate ? identityLeaseParty.expirationDate|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ identityLeaseParty.uploadIdentityPath }}</td>
                <td>
                    <a href="{{ path('app_identity_leaseParty_show', {'id': identityLeaseParty.id}) }}">show</a>
                    <a href="{{ path('app_identity_leaseParty_edit', {'id': identityLeaseParty.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_identity_leaseParty_new') }}">Create new</a>
{% endblock %}
